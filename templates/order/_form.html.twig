{{ form_start(form) }}
<div class="container">
    <div><h1 class="text-center">New order</h1></div>
    <div class="row">
        <div class="col-6">
            {{ form_row(form.state) }}
        </div>
        <div class="col-6">
            {{ form_row(form.customer) }}
        </div>
    </div>
    <div class="row">
        <div class="d-none">
            {{ form_row(form.vehicles) }}
        </div>
    </div>
</div>

{{ form_widget(form) }}
<div id="divBotons" class="d-flex justify-content-center gap-2 m-3">
    <button class="btn btn-success">{{ button_label|default('Save') }}</button>
</div>
{{ form_end(form) }}
